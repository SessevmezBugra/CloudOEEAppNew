{
	"_version": "1.17.0",
	"sap.app": {
		"id": "workerapp.components.oeeapp",
		"type": "application",
		"i18n": "i18n/i18n.properties",
		"title": "{{appTitle}}",
		"description": "{{appDescription}}",
		"applicationVersion": {
			"version": "1.0.0"
		},
		"dataSources": {}
	},
	"sap.ui": {
		"technology": "UI5",
		"deviceTypes": {
			"desktop": true,
			"tablet": true,
			"phone": true
		}
	},
	"sap.ui5": {
		"rootView": {
			"viewName": "workerapp.components.oeeapp.view.App",
			"type": "XML",
			"async": true,
			"id": "app"
		},
		"componentUsages": {
			"rootComponent": {
				"name": "workerapp",
				"settings": {},
				"componentData": {},
				"lazy": true
			}
		},
		"dependencies": {
			"minUI5Version": "1.72",
			"libs": {
				"sap.m": {}
			}
		},
		"models": {
			"i18n": {
				"type": "sap.ui.model.resource.ResourceModel",
				"settings": {
					"bundleName": "workerapp.components.oeeapp.i18n.i18n"
				}
			}
		},
		"resources": {
			"css": [
				{
					"uri": "css/style.css"
				}
			]
		},
		"routing": {
			"config": {
				"routerClass": "sap.m.routing.Router",
				"viewType": "XML",
				"path": "workerapp.components.oeeapp.view",
				"controlId": "app",
				"controlAggregation": "pages",
				"transition": "slide",
				"bypassed": {
					"target": "notFound"
				},
				"async": true
			},
			"routes": [
				{
					"pattern": "",
					"name": "orderManagement",
					"target": [
						"orderManagement"
					]
				},
				{
					"pattern": "orderDetail/{orderId}/{layout}",
					"name": "orderDetail",
					"target": [
						"orderManagement",
						"orderDetail"
					]
				},
				{
					"pattern": "reportProduction",
					"name": "reportProduction",
					"target": "reportProduction",
					"layout": "EndColumnFullScreen"
				}
			],
			"targets": {
				"orderManagement": {
					"viewName": "OrderManagement",
					"controlAggregation": "beginColumnPages"
				},
				"orderDetail": {
					"viewName": "OrderDetail",
					"controlAggregation": "midColumnPages"
				},
				"reportProduction": {
					"viewName": "ReportProduction",
					"controlAggregation": "endColumnPages"
				},
				"notFound": {
					"viewName": "NotFound",
					"controlAggregation": "beginColumnPages"
				}
			}
		}
	}
}
