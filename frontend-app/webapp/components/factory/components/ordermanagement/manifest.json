{
	"_version": "1.17.0",
	"sap.app": {
		"id": "workerapp.components.factory.components.ordermanagement",
		"type": "application",
		"i18n": "i18n/i18n.properties",
		"title": "{{appTitle}}",
		"description": "{{appDescription}}",
		"applicationVersion": {
			"version": "1.0.0"
		},
		"dataSources": {}
	},
	"sap.ui": {
		"technology": "UI5",
		"deviceTypes": {
			"desktop": true,
			"tablet": true,
			"phone": true
		}
	},
	"sap.ui5": {
		"rootView": {
			"viewName": "workerapp.components.factory.components.ordermanagement.view.App",
			"type": "XML",
			"async": true,
			"id": "app"
		},
		"componentUsages": {
			"rootComponent": {
				"name": "workerapp",
				"settings": {},
				"componentData": {},
				"lazy": true
			}
		},
		"dependencies": {
			"minUI5Version": "1.72",
			"libs": {
				"sap.m": {}
			}
		},
		"models": {
			"i18n": {
				"type": "sap.ui.model.resource.ResourceModel",
				"settings": {
					"bundleName": "workerapp.components.factory.components.ordermanagement.i18n.i18n"
				}
			}
		},
		"resources": {
			"css": [
				{
					"uri": "css/style.css"
				}
			]
		},
		"routing": {
			"config": {
				"routerClass": "sap.m.routing.Router",
				"viewType": "XML",
				"path": "workerapp.components.factory.components.ordermanagement.view",
				"controlId": "app",
				"controlAggregation": "pages",
				"transition": "slide",
				"bypassed": {
					"target": "notFound"
				},
				"async": true
			},
			"routes": [
				{
					"pattern": "orderList",
					"name": "orderList",
					"target": [
						"orderList"
					]
				},
				{
					"pattern": "orderDetail/{layout}",
					"name": "orderDetail",
					"target": [
						"orderList",
						"orderDetail"
					]
				}
			],
			"targets": {
				"orderList": {
					"viewName": "OrderList",
					"controlAggregation": "beginColumnPages"
				},
				"orderDetail": {
					"viewName": "OrderDetail",
					"controlAggregation": "midColumnPages"
				},
				"home": {
					"type": "View",
					"id": "home",
					"name": "Home",
					"title": "Home"
				},
				"notFound": {
					"type": "View",
					"id": "notFound",
					"name": "NotFound",
					"transition": "show"
				}
			}
		}
	}
}
