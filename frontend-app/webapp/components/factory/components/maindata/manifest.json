{
	"_version": "1.17.0",
	"sap.app": {
		"id": "workerapp.components.factory.components.maindata",
		"type": "application",
		"i18n": "i18n/i18n.properties",
		"title": "{{appTitle}}",
		"description": "{{appDescription}}",
		"applicationVersion": {
			"version": "1.0.0"
		},
		"dataSources": {}
	},
	"sap.ui": {
		"technology": "UI5",
		"deviceTypes": {
			"desktop": true,
			"tablet": true,
			"phone": true
		}
	},
	"sap.ui5": {
		"rootView": {
			"viewName": "workerapp.components.factory.components.maindata.view.App",
			"type": "XML",
			"async": true,
			"id": "app"
		},
		"componentUsages": {
			"rootComponent": {
				"name": "workerapp",
				"settings": {},
				"componentData": {},
				"lazy": true
			}
		},
		"dependencies": {
			"minUI5Version": "1.72",
			"libs": {
				"sap.m": {}
			}
		},
		"models": {
			"i18n": {
				"type": "sap.ui.model.resource.ResourceModel",
				"settings": {
					"bundleName": "workerapp.components.factory.components.maindata.i18n.i18n"
				}
			}
		},
		"resources": {
			"css": [
				{
					"uri": "css/style.css"
				}
			]
		},
		"routing": {
			"config": {
				"routerClass": "sap.m.routing.Router",
				"viewType": "XML",
				"path": "workerapp.components.factory.components.maindata.view",
				"controlId": "app",
				"controlAggregation": "pages",
				"transition": "slide",
				"bypassed": {
					"target": "notFound"
				},
				"async": true
			},
			"routes": [
				{
					"pattern": "",
					"name": "home",
					"target": "home"
				},
				{
					"pattern": "materiallist",
					"name": "materialList",
					"target": "materialList"
				},
				{
					"pattern": "materialdetail/{action}/{id}",
					"name": "materialDetail",
					"target": "materialDetail"
				},{
					"pattern": "company",
					"name": "company",
					"target": "company"
				},{
					"pattern": "client/{id}",
					"name": "client",
					"target": "client"
				},{
					"pattern": "plant/{id}",
					"name": "plant",
					"target": "plant"
				}
			],
			"targets": {
				"materialList": {
					"type": "View",
					"id": "materialList",
					"name": "MaterialList",
					"title": "MaterialList"
				},
				"materialDetail": {
					"type": "View",
					"id": "materialDetail",
					"name": "MaterialDetail",
					"title": "MaterialDetail"
				},"company": {
					"type": "View",
					"id": "company",
					"name": "Company",
					"title": "Company"
				},"client": {
					"type": "View",
					"id": "client",
					"name": "Client",
					"title": "Client"
				},"plant": {
					"type": "View",
					"id": "plant",
					"name": "Plant",
					"title": "Plant"
				},
				"home": {
					"type": "View",
					"id": "home",
					"name": "Home",
					"title": "Home"
				},
				"notFound": {
					"type": "View",
					"id": "notFound",
					"name": "NotFound",
					"transition": "show"
				}
			}
		}
	}
}
