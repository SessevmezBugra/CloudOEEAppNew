{
	"_version": "1.17.0",
	"sap.app": {
		"id": "workerapp",
		"type": "application",
		"i18n": "i18n/i18n.properties",
		"title": "{{APP_TITLE}}",
		"description": "{{APP_DESC}}",
		"applicationVersion": {
			"version": "1.0.0"
		},
		"dataSources": {}
	},
	"sap.ui": {
		"technology": "UI5",
		"deviceTypes": {
			"desktop": true,
			"tablet": true,
			"phone": true
		}
	},
	"sap.ui5": {
		"rootView": {
			"viewName": "workerapp.view.App",
			"type": "XML",
			"async": true,
			"id": "app"
		},
		"componentUsages": {
			"factoryComponent": {
				"name": "workerapp.components.factory",
				"settings": {},
				"componentData": {},
				"lazy": true
			},
			"homeComponent": {
				"name": "workerapp.components.home",
				"settings": {},
				"componentData": {},
				"lazy": true
			}
		},
		"dependencies": {
			"minUI5Version": "1.72",
			"libs": {
				"sap.m": {}
			}
		},
		"models": {
			"i18n": {
				"type": "sap.ui.model.resource.ResourceModel",
				"settings": {
					"bundleName": "workerapp.i18n.i18n"
				}
			}
		},
		"resources": {
			"css": [
				{
					"uri": "css/style.css"
				}
			]
		},
		"routing": {
			"config": {
				"routerClass": "sap.m.routing.Router",
				"viewType": "XML",
				"path": "workerapp.view",
				"controlId": "app",
				"controlAggregation": "pages",
				"transition": "slide",
				"bypassed": {
					"target": "notFound"
				},
				"async": true,
				"propagateTitle": true
			},
			"routes": [
				{
					"name": "home",
					"pattern": "",
					"target": {
						"name": "home",
						"prefix": "h"
					}
				},
				{
					"name": "factory",
					"pattern": "factory",
					"target": {
						"name": "factory",
						"prefix": "h"
					}
				}
			],
			"targets": {
				"home": {
					"type": "Component",
					"usage": "homeComponent",
					"title": "home"
				},
				"factory": {
					"type": "Component",
					"usage": "factoryComponent",
					"title": "factory"
				},
				"notFound": {
					"type": "View",
					"id": "notFound",
					"name": "NotFound",
					"transition": "show"
				}
			}
		}
	}
}
